{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-case-declarations": "off",
    "no-debugger": "warn",
    "no-empty": "off",
    "no-fallthrough": "off"
  }
}
